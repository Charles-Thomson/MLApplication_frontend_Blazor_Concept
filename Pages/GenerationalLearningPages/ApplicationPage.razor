@page "/generational_learning_application_page"

@using MLApplication_frontend.Components.Navigation.TopNavigationBar
@using MLApplication_frontend.Components.Environment
@using MLApplication_frontend.Layout
@using MLApplication_frontend.Components.SideContentPanel
@inject Services.InstanceAttributeStateContainer stateContainer


<GenerationalLearning_ApplicationLayout >
    <Content_NavigtionBar>
        <TopNavigationBar></TopNavigationBar>
    </Content_NavigtionBar>

    <Content_LeftPanel>
       
        <SideContentPanelTemplate PanelContent="@PanelContent" ButtonGroupType="@ButtonGroupType">           
        </SideContentPanelTemplate>
   </Content_LeftPanel>

    <Content_Main>
        <CascadingValue name="NodeTypeSelectionValue_cascade" Value="@NodeTypeSelectionValue">
            <CascadingValue name="CurrentEnvironmentDimension_X" Value="@CurrentEnvironmentDimension_X">
                <CascadingValue name="CurrentEnvironmentDimension_Y" Value="@CurrentEnvironmentDimension_Y">
                    <ConstructedEnvironment />
                </CascadingValue>
            </CascadingValue>
        </CascadingValue>

    </Content_Main>

</GenerationalLearning_ApplicationLayout>






